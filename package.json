{
  "name": "curso-protractor-new",
  "version": "1.0.0",
  "description": "Projeto para o curso de Protractor da Escola TAT",
  "scripts": {
    "lint": "npx standard",
    "lint:fix": "npx standard --fix",
    "pretest": "webdriver-manager update",
    "test": "npm run test:chrome && npm run test:firefox",
    "test:chrome": "protractor configurations/chromeConfig.js",
    "test:firefox": "protractor configurations/firefoxConfig.js"
  },
  "standard": {
    "globals": [
      "beforeAll",
      "beforeEach",
      "browser",
      "by",
      "describe",
      "element",
      "expect",
      "it"
    ]
  },
  "keywords": [
    "protractor",
    "testing",
    "e2e"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wlsf82/curso-protractor-tat"
  },
  "author": "Walmyr Filho <wlsf82@gmail.com> (https://walmyr-filho.com)",
  "license": "MIT",
  "devDependencies": {
    "faker": "^4.1.0",
    "protractor": "^7.0.0",
    "protractor-helper": "^4.1.7",
    "standardjs": "^1.0.0-alpha"
  }
}
